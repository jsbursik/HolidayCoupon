		"@sveltejs/adapter-auto": "^6.0.1",
		"@sveltejs/adapter-cloudflare": "^7.1.2",
		"@sveltejs/kit": "^2.26.1",
		"@sveltejs/vite-plugin-svelte": "^6.1.0",
		"@types/node": "^22.17.0",
		"drizzle-kit": "^0.30.6",
		"eslint": "^9.32.0",
		"eslint-plugin-svelte": "^3.11.0",
		"globals": "^16.3.0",
		"node-email-verifier": "^3.4.1",
		"svelte": "^5.37.1",
		"svelte-check": "^4.3.0",
